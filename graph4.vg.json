{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "Household types over time.",
    "data": {"url": "https://raw.githubusercontent.com/MonashAmy/FIT3179-Data-Visualisation-2/refs/heads/main/data/data4.csv"},
    "mark": "line",
    "encoding": {
      "x": {
        "field": "Year",
        "type": "temporal",
        "title": "Year"
      },
      "y": {
        "field": "Households",
        "type": "quantitative",
        "title": "Number of Households"
      },
      "color": {
        "field": "Type of Households",
        "type": "nominal",
        "title": "Type of Households"
      }
    }
  }